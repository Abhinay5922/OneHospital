# One Hospital Frontend - Production Environment Variables
# Copy this file to .env.production and update values

# API URL - Leave blank if hosting frontend and backend on the same domain
# Example: https://api.your-domain.com/api
REACT_APP_API_URL=/api

# Socket Server URL - Leave blank if hosting on the same domain
# Example: https://api.your-domain.com
REACT_APP_SERVER_URL=

# Performance
GENERATE_SOURCEMAP=false
